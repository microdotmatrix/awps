{% extends "layouts/base.twig" %}

{% block content %}

	<section id="blog">
		{% for post in posts %}
			{% include ['partials/tease-'~post.post_type~'.twig', 'partials/tease.twig'] %}
		{% endfor %}
	</section>

	{% include 'partials/pagination.twig' with { pagination: posts.pagination({show_all: false, mid_size: 3, end_size: 2}) } %}
{% endblock %}
