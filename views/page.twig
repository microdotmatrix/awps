{% extends "layouts/base.twig" %}

{% set home = value %}

{% block content %}
	<article class="post-type-{{post.post_type}}" id="{{post.slug}}">
		<section class="article-content">
			<h1 class="post-title">{{post.title}}</h1>
			<div class="article-body">
				{{post.content}}
			</div>
		</section>
	</article>
{% endblock %}
